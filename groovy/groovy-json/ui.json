{
  "items":[
    {
      "type":"chart",
      "height":270,
      "width":319,
      "animate":true,
      "insetPadding":20,
      "axes":[
        {
          "type":"Time",
          "fields":[ "x" ],
          "position":"left",
          "title":"Time"
        },
        {
          "type":"Numeric",
          "fields":[ "y" ],
          "position":"bottom",
          "title":"Profit in EUR"
        }
      ],
      "series":[
        {
          "type":"bar",
          "label":{
            "display":"insideEnd",
            "field":"y",
            "color":"#333",
            "text-anchor":"middle"
          },
          "axis":"bottom",
          "xField":"x",
          "yField":[ "y" ]
        }
      ]
    }
  ]
}